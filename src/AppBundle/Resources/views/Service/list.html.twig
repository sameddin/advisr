{% extends 'layout.html.twig' %}

{% block content %}

        <h2>{{ 'service.list'|trans }}</h2>
        {% for service in services %}
            <div class="panel panel-default">
                <div class="panel-heading">{{ service.title }}</div>
                <div class="panel-body">{{ service.description }}</div>
            </div>
        {% endfor %}

{% endblock %}
